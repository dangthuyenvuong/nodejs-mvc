doctype html
head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(http-equiv='Content-Language' content='en')
    meta(http-equiv='Content-Type' content='text/html; charset=utf-8')
    title
        | Analytics Dashboard - This is an example dashboard created using build-in elements and components.
    meta(name='viewport' content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no')
    meta(name='description' content='This is an example dashboard created using build-in elements and components.')
    // Disable tap highlight on IE
    meta(name='msapplication-tap-highlight' content='no')
    link(href='/backend/assets/css/mainstyle.css' rel='stylesheet')
.app-container.app-theme-white.body-tabs-shadow.fixed-header.fixed-sidebar
    include header
    include setting
    .app-main
        include sidebar
        .app-main__outer
            .app-main__inner
                block content
            include footer
.app-drawer-wrapper
    .drawer-nav-btn
        button.hamburger.hamburger--elastic.is-active(type='button')
            span.hamburger-box
                span.hamburger-inner
    .drawer-content-wrapper
        .scrollbar-container
            h3.drawer-heading Servers Status
            .drawer-section
                .row
                    .col
                        .progress-box
                            h4 Server Load 1
                            .circle-progress.circle-progress-gradient-xl.mx-auto
                                small
                    .col
                        .progress-box
                            h4 Server Load 2
                            .circle-progress.circle-progress-success-xl.mx-auto
                                small
                    .col
                        .progress-box
                            h4 Server Load 3
                            .circle-progress.circle-progress-danger-xl.mx-auto
                                small
                .divider
                .mt-3
                    h5.text-center.card-title Live Statistics
                    #sparkline-carousel-3
                    .row
                        .col
                            .widget-chart.p-0
                                .widget-chart-content
                                    .widget-numbers.text-warning.fsize-3 43
                                    .widget-subheading.pt-1 Packages
                        .col
                            .widget-chart.p-0
                                .widget-chart-content
                                    .widget-numbers.text-danger.fsize-3 65
                                    .widget-subheading.pt-1 Dropped
                        .col
                            .widget-chart.p-0
                                .widget-chart-content
                                    .widget-numbers.text-success.fsize-3 18
                                    .widget-subheading.pt-1 Invalid
                    .divider
                    .text-center.mt-2.d-block
                        button.mr-2.border-0.btn-transition.btn.btn-outline-danger Escalate Issue
                        button.border-0.btn-transition.btn.btn-outline-success Support Center
            h3.drawer-heading File Transfers
            .drawer-section.p-0
                .files-box
                    ul.list-group.list-group-flush
                        li.pt-2.pb-2.pr-2.list-group-item
                            .widget-content.p-0
                                .widget-content-wrapper
                                    .widget-content-left.opacity-6.fsize-2.mr-3.text-primary.center-elem
                                        i.fa.fa-file-alt
                                    .widget-content-left
                                        .widget-heading.font-weight-normal TPSReport.docx
                                    .widget-content-right.widget-content-actions
                                        button.btn-icon.btn-icon-only.btn.btn-link.btn-sm
                                            i.fa.fa-cloud-download-alt
                        li.pt-2.pb-2.pr-2.list-group-item
                            .widget-content.p-0
                                .widget-content-wrapper
                                    .widget-content-left.opacity-6.fsize-2.mr-3.text-warning.center-elem
                                        i.fa.fa-file-archive
                                    .widget-content-left
                                        .widget-heading.font-weight-normal Latest_photos.zip
                                    .widget-content-right.widget-content-actions
                                        button.btn-icon.btn-icon-only.btn.btn-link.btn-sm
                                            i.fa.fa-cloud-download-alt
                        li.pt-2.pb-2.pr-2.list-group-item
                            .widget-content.p-0
                                .widget-content-wrapper
                                    .widget-content-left.opacity-6.fsize-2.mr-3.text-danger.center-elem
                                        i.fa.fa-file-pdf
                                    .widget-content-left
                                        .widget-heading.font-weight-normal Annual Revenue.pdf
                                    .widget-content-right.widget-content-actions
                                        button.btn-icon.btn-icon-only.btn.btn-link.btn-sm
                                            i.fa.fa-cloud-download-alt
                        li.pt-2.pb-2.pr-2.list-group-item
                            .widget-content.p-0
                                .widget-content-wrapper
                                    .widget-content-left.opacity-6.fsize-2.mr-3.text-success.center-elem
                                        i.fa.fa-file-excel
                                    .widget-content-left
                                        .widget-heading.font-weight-normal
                                            | Analytics_GrowthReport.xls
                                    .widget-content-right.widget-content-actions
                                        button.btn-icon.btn-icon-only.btn.btn-link.btn-sm
                                            i.fa.fa-cloud-download-alt
            h3.drawer-heading Tasks in Progress
            .drawer-section.p-0
                .todo-box
                    ul.todo-list-wrapper.list-group.list-group-flush
                        li.list-group-item
                            .todo-indicator.bg-warning
                            .widget-content.p-0
                                .widget-content-wrapper
                                    .widget-content-left.mr-2
                                        .custom-checkbox.custom-control
                                            input#exampleCustomCheckbox1266.custom-control-input(type='checkbox')
                                            label.custom-control-label(for='exampleCustomCheckbox1266') &nbsp;
                                    .widget-content-left
                                        .widget-heading
                                            | Wash the car
                                            .badge.badge-danger.ml-2 Rejected
                                        .widget-subheading
                                            i Written by Bob
                                    .widget-content-right.widget-content-actions
                                        button.border-0.btn-transition.btn.btn-outline-success
                                            i.fa.fa-check
                                        button.border-0.btn-transition.btn.btn-outline-danger
                                            i.fa.fa-trash-alt
                        li.list-group-item
                            .todo-indicator.bg-focus
                            .widget-content.p-0
                                .widget-content-wrapper
                                    .widget-content-left.mr-2
                                        .custom-checkbox.custom-control
                                            input#exampleCustomCheckbox1666.custom-control-input(type='checkbox')
                                            label.custom-control-label(for='exampleCustomCheckbox1666') &nbsp;
                                    .widget-content-left
                                        .widget-heading Task with hover dropdown menu
                                        .widget-subheading
                                            div
                                                | By Johnny
                                                .badge.badge-pill.badge-info.ml-2 NEW
                                    .widget-content-right.widget-content-actions
                                        .d-inline-block.dropdown
                                            button.border-0.btn-transition.btn.btn-link(type='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                                                i.fa.fa-ellipsis-h
                                            .dropdown-menu.dropdown-menu-right(tabindex='-1' role='menu' aria-hidden='true')
                                                h6.dropdown-header(tabindex='-1') Header
                                                button.disabled.dropdown-item(type='button' disabled='' tabindex='-1') Action
                                                button.dropdown-item(type='button' tabindex='0')
                                                    | Another
                                                    | Action
                                                .dropdown-divider(tabindex='-1')
                                                button.dropdown-item(type='button' tabindex='0')
                                                    | Another
                                                    | Action
                        li.list-group-item
                            .todo-indicator.bg-primary
                            .widget-content.p-0
                                .widget-content-wrapper
                                    .widget-content-left.mr-2
                                        .custom-checkbox.custom-control
                                            input#exampleCustomCheckbox4777.custom-control-input(type='checkbox')
                                            label.custom-control-label(for='exampleCustomCheckbox4777') &nbsp;
                                    .widget-content-left.flex2
                                        .widget-heading Badge on the right task
                                        .widget-subheading This task has show on hover actions!
                                    .widget-content-right.widget-content-actions
                                        button.border-0.btn-transition.btn.btn-outline-success
                                            i.fa.fa-check
                                    .widget-content-right.ml-3
                                        .badge.badge-pill.badge-success Latest Task
                        li.list-group-item
                            .todo-indicator.bg-info
                            .widget-content.p-0
                                .widget-content-wrapper
                                    .widget-content-left.mr-2
                                        .custom-checkbox.custom-control
                                            input#exampleCustomCheckbox2444.custom-control-input(type='checkbox')
                                            label.custom-control-label(for='exampleCustomCheckbox2444') &nbsp;
                                    .widget-content-left.mr-3
                                        .widget-content-left
                                            img.rounded(width='42' src='/backend/assets/images/avatars/1.jpg' alt='')
                                    .widget-content-left
                                        .widget-heading Go grocery shopping
                                        .widget-subheading A short description ...
                                    .widget-content-right.widget-content-actions
                                        button.border-0.btn-transition.btn.btn-sm.btn-outline-success
                                            i.fa.fa-check
                                        button.border-0.btn-transition.btn.btn-sm.btn-outline-danger
                                            i.fa.fa-trash-alt
                        li.list-group-item
                            .todo-indicator.bg-success
                            .widget-content.p-0
                                .widget-content-wrapper
                                    .widget-content-left.mr-2
                                        .custom-checkbox.custom-control
                                            input#exampleCustomCheckbox3222.custom-control-input(type='checkbox')
                                            label.custom-control-label(for='exampleCustomCheckbox3222') &nbsp;
                                    .widget-content-left.flex2
                                        .widget-heading Development Task
                                        .widget-subheading Finish React ToDo List App
                                    .widget-content-right
                                        .badge.badge-warning.mr-2 69
                                    .widget-content-right
                                        button.border-0.btn-transition.btn.btn-outline-success
                                            i.fa.fa-check
                                        button.border-0.btn-transition.btn.btn-outline-danger
                                            i.fa.fa-trash-alt
            h3.drawer-heading Urgent Notifications
            .drawer-section
                .notifications-box
                    .vertical-time-simple.vertical-without-time.vertical-timeline.vertical-timeline--one-column
                        .vertical-timeline-item.dot-danger.vertical-timeline-element
                            div
                                span.vertical-timeline-element-icon.bounce-in
                                .vertical-timeline-element-content.bounce-in
                                    h4.timeline-title All Hands Meeting
                                    span.vertical-timeline-element-date
                        .vertical-timeline-item.dot-warning.vertical-timeline-element
                            div
                                span.vertical-timeline-element-icon.bounce-in
                                .vertical-timeline-element-content.bounce-in
                                    p
                                        | Yet another one, at 
                                        span.text-success 15:00 PM
                                    span.vertical-timeline-element-date
                        .vertical-timeline-item.dot-success.vertical-timeline-element
                            div
                                span.vertical-timeline-element-icon.bounce-in
                                .vertical-timeline-element-content.bounce-in
                                    h4.timeline-title
                                        | Build the production release
                                        .badge.badge-danger.ml-2 NEW
                                    span.vertical-timeline-element-date
                        .vertical-timeline-item.dot-primary.vertical-timeline-element
                            div
                                span.vertical-timeline-element-icon.bounce-in
                                .vertical-timeline-element-content.bounce-in
                                    h4.timeline-title
                                        | Something not important
                                        .avatar-wrapper.mt-2.avatar-wrapper-overlap
                                            .avatar-icon-wrapper.avatar-icon-sm
                                                .avatar-icon
                                                    img(src='/backend/assets/images/avatars/1.jpg' alt='')
                                            .avatar-icon-wrapper.avatar-icon-sm
                                                .avatar-icon
                                                    img(src='/backend/assets/images/avatars/2.jpg' alt='')
                                            .avatar-icon-wrapper.avatar-icon-sm
                                                .avatar-icon
                                                    img(src='/backend/assets/images/avatars/3.jpg' alt='')
                                            .avatar-icon-wrapper.avatar-icon-sm
                                                .avatar-icon
                                                    img(src='/backend/assets/images/avatars/4.jpg' alt='')
                                            .avatar-icon-wrapper.avatar-icon-sm
                                                .avatar-icon
                                                    img(src='/backend/assets/images/avatars/5.jpg' alt='')
                                            .avatar-icon-wrapper.avatar-icon-sm
                                                .avatar-icon
                                                    img(src='/backend/assets/images/avatars/6.jpg' alt='')
                                            .avatar-icon-wrapper.avatar-icon-sm
                                                .avatar-icon
                                                    img(src='/backend/assets/images/avatars/7.jpg' alt='')
                                            .avatar-icon-wrapper.avatar-icon-sm
                                                .avatar-icon
                                                    img(src='/backend/assets/images/avatars/8.jpg' alt='')
                                            .avatar-icon-wrapper.avatar-icon-sm.avatar-icon-add
                                                .avatar-icon
                                                    i +
                                    span.vertical-timeline-element-date
                        .vertical-timeline-item.dot-info.vertical-timeline-element
                            div
                                span.vertical-timeline-element-icon.bounce-in
                                .vertical-timeline-element-content.bounce-in
                                    h4.timeline-title This dot has an info state
                                    span.vertical-timeline-element-date
                        .vertical-timeline-item.dot-dark.vertical-timeline-element
                            div
                                span.vertical-timeline-element-icon.is-hidden
                                .vertical-timeline-element-content.is-hidden
                                    h4.timeline-title This dot has a dark state
                                    span.vertical-timeline-element-date
.app-drawer-overlay.d-none.animated.fadeIn
script(type='text/javascript' src='/backend/assets/js/mainscript.js')
